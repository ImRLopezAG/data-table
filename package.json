{
	"name": "cloudflare",
	"type": "module",
	"scripts": {
		"dev": "bun run clear && bun run build:r && vite",
		"build": "vite build && bun run build:r",
		"preview": "bun run clear && $npm_execpath run build && vite preview",
		"deploy": "$npm_execpath run build && wrangler deploy",
		"cf-typegen": "wrangler types --env-interface CloudflareBindings",
		"build:r": "bunx shadcn build -o ./dist/client/assets/",
		"gen": "tsr generate",
		"clear": "rm -rf dist .wrangler",
		"lint": "biome check --write --unsafe ."
	},
