{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "data-table",
  "main": "src/index.ts",
  "compatibility_date": "2025-04-25",
  "compatibility_flags": [
    "nodejs_compat_v2",
  ],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "assets": {
    "directory": "./dist/client/",
    "binding": "ASSETS"
  },
  "route" : {
    "pattern": "components.imrlopez.dev/*",
    "zone_name": "imrlopez.dev"
  }
}
